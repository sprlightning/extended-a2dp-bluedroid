<node>

	<!-- NOTE: Due to a bug in gdbus-codegen in glib prior to 2.61.0,
	           the first interface must have at least one method.
	-->

	<interface name="org.ofono.HandsfreeAudioAgent">
		<method name="NewConnection">
			<arg direction="in" type="o" name="card"/>
			<arg direction="in" type="h" name="fd"/>
			<arg direction="in" type="y" name="codec"/>
		</method>
		<method name="Release">
		</method>
	</interface>

</node>
